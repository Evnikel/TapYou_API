<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="api.UserGenderList" level="info" additivity="false">
            <AppenderRef ref="Console" />
        </Logger>
        <Logger name="api.UserGenderAnd_ID" level="info" additivity="false">
            <AppenderRef ref="Console" />
        </Logger>
        <Logger name="api.UserID" level="info" additivity="false">
            <AppenderRef ref="Console" />
        </Logger>
    </Loggers>
</Configuration>


